        <tei:p xml:id="o-p2">
          <tei:ptr xml:id="o-p2-1" target="#se3"/>
        </tei:p>
      </j:view>
    </j:concurrent>
    <j:repository>
      <tei:seg xml:id="r1">אחת</tei:seg>
      <tei:seg xml:id="r2">ושתים</tei:seg>
      <tei:seg xml:id="r3">שלשה</tei:seg>
    </j:repository>
  </tei:text>
</tei:TEI>


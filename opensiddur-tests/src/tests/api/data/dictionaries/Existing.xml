               </tei:sense>
               <tei:sense xml:id="H1S4b">
                  <tei:def>of people</tei:def>
               </tei:sense>
               <tei:sense xml:id="H1S5">
                  <tei:def>originator or patron of a class,  profession,  or art</tei:def>
               </tei:sense>
               <tei:sense xml:id="H1S6">
                  <tei:def>of producer,  generator (fig.)</tei:def>
               </tei:sense>
               <tei:sense xml:id="H1S7">

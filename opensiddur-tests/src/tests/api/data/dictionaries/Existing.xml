            <tei:cit type="translation">
               <tei:quote type="strongs">
                  <tei:hi>father</tei:hi>, in a literal and immediate, or figurative and remote application</tei:quote>
               <tei:quote type="kjv">chief, (fore-) father(-less), [idiom] patrimony, principal. Compare names in 'Abi-'.</tei:quote>
            </tei:cit>
            <tei:sense>
               <tei:sense xml:id="QueryResult">
                  <tei:def>Query result</tei:def>
               </tei:sense>

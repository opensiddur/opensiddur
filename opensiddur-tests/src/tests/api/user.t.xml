         <xpath desc="profile owned by group everyone">sm:get-permissions(xs:anyURI("/db/data/user/deltestuser2.xml"))/*/@group="everyone"</xpath>
         <xpath desc="profile permissions are 664">sm:get-permissions(xs:anyURI("/db/data/user/deltestuser2.xml"))/*/@mode="rw-rw-r--"</xpath>

  <TestSet>
      <testName>user:delete(): authenticated and an external reference exists</testName>
      <setup>
         <copy source="/db/apps/opensiddur-tests/tests/api/user" name="Reference.xml" 
            destination="/db/data/original"/>
         <code>
      system:as-user("admin", $magic:password, (
        let $u := sm:create-account("deltestuser2", "deltestuser2", "everyone")
        let $s := xmldb:store("/db/data/user", "deltestuser2.xml",
          &lt;j:contributor&gt;
            &lt;tei:idno&gt;deltestuser2&lt;/tei:idno&gt;
          &lt;/j:contributor&gt;
          )
        let $uri := xs:anyURI("/db/data/user/deltestuser2.xml")
        let $doc-uri := xs:anyURI("/db/data/original/Reference.xml")
        let $ch := (
          sm:chown($uri, "deltestuser2"),
          sm:chgrp($uri, "deltestuser2"),
          sm:chmod($uri, "rw-r--r--"),
          sm:chown($doc-uri, "deltestuser2"),
          sm:chgrp($doc-uri, "deltestuser2"),
          sm:chmod($doc-uri, "rw-rw-r--"),
          ridx:reindex(doc("/db/data/original/Reference.xml"))
        )
        return ()
      ))
      </code>
      </setup>
      <tearDown>
         <remove-document collection="/db/data/original" name="Reference.xml"/>
         <code>
      system:as-user("admin", $magic:password, (
        if (doc-available("/db/data/user/deltestuser2.xml"))
        then xmldb:remove("/db/data/user", "deltestuser2.xml")
        else (),
        if (sm:user-exists("deltestuser2"))
        then sm:remove-account("deltestuser2")
        else (),
        if (sm:group-exists("deltestuser2"))
        then sm:remove-group("deltestuser2")
        else ()
      ))
      </code>
      </tearDown>
      <test>
         <task>Delete your own profile</task>
         <code>
      system:as-user("deltestuser2","deltestuser2", (
        let $ret := user:delete("deltestuser2")
             let $log := util:log("info", ("***deletion of deltestuser returns =",  $ret))
             return $ret
      ))
      </code>
         <xpath desc="status code is 200">self::rest:response/http:response/@status = 200</xpath>
         <xpath desc="returns an info element with the referenced document">ends-with(self::info//documents/document, "/api/data/original/Reference")</xpath>
         <xpath desc="user account removed">not(sm:user-exists("deltestuser2"))</xpath>

  <TestSet>
      <testName>user:post-xml(): unauthenticated</testName>
      <asUser/>
      <password/>
      <tearDown>
      <!-- remove user, group, and profile -->
      <code>
      system:as-user("admin", $magic:password,(
        if (sm:group-exists("usertestuser"))
        then 
          try {
            sm:remove-group("usertestuser")
          } 
          catch * { 
            util:log-system-out((
    $err:module, ":", $err:line-number, ":", 
    $err:column-number, ","[string($err:code)], $err:code, ":"[string($err:value)], $err:value, ": "[$err:description],
    $err:description,
    ";")) 
          }
        else (),
        if (sm:user-exists("usertestuser"))
        then sm:remove-account("usertestuser")
        else (),
        if (doc-available("/db/data/user/usertestuser.xml"))
        then xmldb:remove("/db/data/user", "usertestuser.xml")
        else ()
      ))
      </code>
      </tearDown>
      <test>
         <task>Create a new user</task>
         <code><![CDATA[
      user:post-xml(document {
        <new>
          <user>usertestuser</user>
          <password>usertestuser</password>
        </new>
      })
      ]]></code>
         <class href="common.t.incl.xml#Created"/>
         <xpath desc="user has been created">system:as-user("admin", $magic:password, sm:user-exists("usertestuser"))</xpath>
         <xpath desc="group has been created">system:as-user("admin", $magic:password, sm:group-exists("usertestuser"))</xpath>
         <xpath desc="user is a manager of its own group">system:as-user("admin", $magic:password, sm:get-group-managers("usertestuser")="usertestuser")</xpath>
         <xpath desc="profile has been created">doc-available("/db/data/user/usertestuser.xml")</xpath>
         <xpath desc="profile contains an idno equal to the user name">doc("/db/data/user/usertestuser.xml")/j:contributor/tei:idno="usertestuser"</xpath>
         <xpath desc="user profile permissions are correct">
      doc-available("/db/data/user/usertestuser.xml") and
      sm:get-permissions(xs:anyURI("/db/data/user/usertestuser.xml"))/*/(
        @owner = "usertestuser" and @group="usertestuser" and @mode="rw-r--r--"
      )</xpath>
      </test>

<?xml version="1.0" encoding="UTF-8"?>
<tei:TEI xmlns:tei="http://www.tei-c.org/ns/1.0">
  <tei:teiHeader>
      <tei:publicationStmt>
         <tei:availability>
            <tei:licence target="http://www.creativecommons.org/publicdomain/zero/1.0"/>
         </tei:availability> 
      </tei:publicationStmt>
  </tei:teiHeader>
  <j:settings xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0">
